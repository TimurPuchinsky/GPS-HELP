<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.ProfileFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textViewPassport"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewBirth" />

        <TextView
            android:id="@+id/textViewBirth"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="32dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewFatherName" />

        <TextView
            android:id="@+id/textViewSnils"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewPassport" />

        <TextView
            android:id="@+id/textViewLiveplace"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="40dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewPassport" />

        <TextView
            android:id="@+id/textViewWorkplace"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewLiveplace" />

        <TextView
            android:id="@+id/textViewEmail"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="40dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewLiveplace" />


        <TextView
            android:id="@+id/textViewFatherName"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewName" />

        <TextView
            android:id="@+id/textViewName"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewFamily" />

        <TextView
            android:id="@+id/textViewFamily"
            android:layout_width="227dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonRedact"
            android:layout_width="140sp"
            android:layout_height="80sp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:text="Button"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewEmail" />

        <Button
            android:id="@+id/buttonChangeUser"
            android:layout_width="140sp"
            android:layout_height="80sp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="32dp"
            android:text="ИЗМЕНИТЬ ПОЛЬЗОВАТЕЛЯ"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewEmail"
            tools:ignore="UnknownIdInLayout" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>